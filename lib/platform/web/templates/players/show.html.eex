<h2>Show Players</h2>

<ul>

  <li>
    <strong>ID:</strong>
    <%= @players.id %>
  </li>

  <li>
    <strong>Display Name:</strong>
    <%= @players.display_name %>
  </li>

  <li>
    <strong>Username:</strong>
    <%= @players.username %>
  </li>

  <li>
    <strong>Score:</strong>
    <%= @players.score %>
  </li>

</ul>

<span><%= link "Edit", to: players_path(@conn, :edit, @players) %></span>
<span><%= link "Back", to: players_path(@conn, :index) %></span>
